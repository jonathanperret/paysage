{
  "name": "paysage",
  "version": "0.0.1",
  "engines": {
    "node": "~6.11.2"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "postinstall": "bower install",
    "test": "npm run --silent lint && testem ci -p 0 -l phantomjs && mocha && nightwatch",
    "testem": "testem -l phantomjs",
    "testServer": "mocha",
    "test-e2e": "nightwatch",
    "lint": "semistandard",
    "update-favicon": "real-favicon generate faviconDescription.json faviconData.json public && node -e \"require('fs').writeFileSync('./views/partials/favicon.hbs', require('./faviconData.json').favicon.html_code)\""
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "bower": "^1.5.3",
    "chance": "^1.0.3",
    "cookie-parser": "~1.4.2",
    "debug": "~2.2.0",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "less-middleware": "2.1.x",
    "morgan": "~1.7.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chromedriver": "^2.31.0",
    "cli-real-favicon": "0.0.6",
    "mocha": "^3.5.0",
    "nightwatch": "^0.9.16",
    "phantomjs-prebuilt": "^2.1.7",
    "semistandard": "^11.0.0",
    "sinon": "^3.1.0",
    "sinon-chai": "^2.12.0",
    "socket.io-client": "^2.0.3",
    "supertest": "^3.0.0",
    "testem": "^1.7.1"
  },
  "semistandard": {
  }
}
