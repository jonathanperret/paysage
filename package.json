    "express-session": "^1.15.3",
    "less-middleware": "2.1.x",
    "morgan": "~1.7.0",
    "socket.io": "^2.0.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "cli-real-favicon": "0.0.6",
    "jasmine-node": "^1.14.5",
    "phantomjs-prebuilt": "^2.1.7",
