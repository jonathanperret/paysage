{
  "name": "paysage",
  "version": "0.0.1",
  "engines": {
    "node": "^10.11.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "postinstall": "bower install",
    "test": "npm run --silent lint && testem ci -p 0 -l 'headless chrome' && mocha && nightwatch",
    "testem": "testem -l chrome",
    "testServer": "mocha",
    "test-e2e": "nightwatch",
    "lint": "semistandard",
    "update-favicon": "npx cli-real-favicon generate faviconDescription.json faviconData.json public && node -e \"require('fs').writeFileSync('./views/partials/favicon.hbs', require('./faviconData.json').favicon.html_code)\""
  },
  "dependencies": {
    "aws-sdk": "^2.353.0",
    "body-parser": "^1.18.3",
    "bower": "^1.5.3",
    "chance": "^1.0.3",
    "cookie-parser": "~1.4.2",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "less-middleware": "^3.0.0",
    "morgan": "^1.9.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chromedriver": "^2.43.1",
    "mocha": "^5.2.0",
    "nightwatch": "^1.0.11",
    "semistandard": "^13.0.1",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "socket.io-client": "^2.1.1",
    "supertest": "^3.3.0",
    "testem": "^2.14.0"
  },
  "semistandard": {}
}
